# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'MinCloud' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MinCloud
  pod 'Moya'
  pod 'WechatOpenSDK'
  pod 'AlipaySDK-iOS'

  target 'MinCloudTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
    pod 'Alamofire'
   pod 'WechatOpenSDK'
   pod 'AlipaySDK-iOS'
  end

end

target 'Demo_Swift' do
  use_frameworks!
  platform :ios, '9.0'
  pod 'Moya'
  pod 'WechatOpenSDK'
  pod 'AlipaySDK-iOS'
end

target 'Demo_OC' do
  use_frameworks!
  platform :ios, '9.0'
  pod 'Moya'
  pod 'WechatOpenSDK'
  pod 'AlipaySDK-iOS'
end
